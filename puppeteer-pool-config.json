{
  "browser_pool": {
    "min": 2,
    "max": 5
  },
  "session_pool": {
    "min": 3,
    "max": 5,
    "width": 100,
    "height": 100,
    "ignoreResourceLoad": true,
    "enablePageCache": true
  },
  "threshold": {
    "activate": true,
    "interval": 5,
    "cpu": {
      "break": 30,
      "warn": 20
    },
    "memory": {
      "break": 600,
      "warn": 400
    }
  }
}
