{
  "browser_pool": {
    "min": 2,
    "max": 5
  },
  "session_pool": {
    "min": 3,
    "max": 5,
    "width": 100,
    "height": 100,
    "ignoreResourceLoad": true,
    "enablePageCache": true
  },
  "threshold": {
    "interval": 5,
    "cpu": {
      "break": 10,
      "warn": 5
    },
    "memory": {
      "break": 300,
      "warn": 200
    }
  }
}
