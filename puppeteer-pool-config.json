{
  "browser_pool": {
    "min": 2,
    "max": 3
  },
  "session_pool": {
    "min": 10,
    "max": 40,
    "width": 1000,
    "height": 1000,
    "ignoreResourceLoad": true,
    "enablePageCache": true
  },
  "threshold": {
    "activate": true,
    "interval": 10,
    "cpu": {
      "break": 30,
      "warn": 20
    },
    "memory": {
      "break": 500,
      "warn": 200
    }
  }
}
