{
  "browser_pool": {
    "min": 2,
    "max": 3
  },
  "session_pool": {
    "min": 5,
    "max": 7,
    "width": 1500,
    "height": 1500,
    "ignoreResourceLoad": true,
    "enablePageCache": false
  },
  "threshold": {
    "activate": true,
    "interval": 10,
    "cpu": {
      "break": 90,
      "warn": 60
    },
    "memory": {
      "break": 2048,
      "warn": 900
    }
  }
}
